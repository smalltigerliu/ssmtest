<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.opaltest.dao.CityDao">
    <insert id="insertCity">
        insert into city(name, provinceid) values (#{name}, #{provinceId})
    </insert>

    <select id="selectCityByProvinceId" resultType="com.opaltest.domain.City">
        select id, name, provinceid from city where provinceid=#{pid}
    </select>
</mapper>